	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@material-ui/core": "3.9.3",
		"@material-ui/icons": "3.0.2",
		"query-string": "6.5.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-move": "5.2.1",
		"react-scripts": "^2.1.8",
